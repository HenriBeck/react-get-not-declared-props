{
  "name": "react-get-not-declared-props",
  "version": "1.0.0",
  "description": "Utility function to get all additional props that were passed to a component",
  "main": "lib/index.js",
  "repository": "https://github.com/HenriBeck/react-get-not-declared-props.git",
  "author": "Henri Beck <henribeck.dev@gmail.com>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "ava",
    "test:coverage": "cross-env NODE_ENV=test nyc yarn test",
    "posttest:coverage": "yarn generate-report",
    "prepare": "yarn build",
    "prebuild": "rimraf lib/",
    "build": "yarn build:lib && yarn build:flow",
    "build:lib": "rimraf lib/ && babel src -d lib --ignore=\"*.test.js\"",
    "build:flow": "flow-copy-source src lib -i *.test.js",
    "generate-report": "nyc report --reporter=html",
    "lint": "eslint src/",
    "flow": "flow check ./src/"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.54",
    "@babel/core": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/preset-flow": "7.0.0-beta.54",
    "@babel/register": "7.0.0-beta.54",
    "ava": "1.0.0-beta.6",
    "babel-plugin-istanbul": "4.1.6",
    "cross-env": "5.2.0",
    "eslint": "5.1.0",
    "eslint-config-henribeck": "5.1.1",
    "flow-bin": "0.76.0",
    "flow-copy-source": "2.0.1",
    "nyc": "12.0.2",
    "react": "^16.4.1",
    "rimraf": "2.6.2"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "check-coverage": true,
    "report-dir": "./.coverage",
    "exclude": [
      "*.config.js",
      "*.test.js"
    ]
  }
}
