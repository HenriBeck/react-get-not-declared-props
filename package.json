{
  "name": "react-get-not-declared-props",
  "version": "1.0.0",
  "description": "Utility function to get all additional props that were passed to a component",
  "main": "lib/index.js",
  "repository": "https://github.com/HenriBeck/react-get-not-declared-props.git",
  "author": "Henri Beck <henribeck.dev@gmail.com>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "ava",
    "test:coverage": "nyc yarn run test",
    "prebuild": "rimraf lib/",
    "build": "babel src -d lib --ignore=\"*.spec.js\" && flow-copy-source src lib -i *.spec.js",
    "generate-report:html": "nyc report --reporter=html",
    "generate-report:lcov": "nyc report --reporter=text-lcov",
    "report-coverage": "yarn run generate-report:lcov | coveralls",
    "prepublish": "yarn run build",
    "lint": "eslint src/",
    "flow": "flow check ./src/"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.51",
    "@babel/core": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "@babel/preset-flow": "7.0.0-beta.51",
    "@babel/register": "7.0.0-beta.51",
    "ava": "1.0.0-beta.6",
    "eslint": "4.19.1",
    "eslint-config-henribeck": "4.19.7",
    "flow-bin": "0.75.0",
    "flow-copy-source": "2.0.0",
    "nyc": "12.0.2",
    "rimraf": "2.6.2"
  },
  "ava": {
    "cache": false,
    "files": [
      "src/index.spec.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "exclude": [
      "src/index.spec.js",
      ".babelrc.js"
    ],
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "report-dir": "./.coverage"
  }
}
