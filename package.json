{
  "name": "react-get-not-declared-props",
  "version": "0.1.1",
  "description": "Utility function to get all extra props that were passed to a component",
  "main": "lib/get-not-declared.js",
  "repository": "https://github.com/HenriBeck/react-get-not-declared-props.git",
  "author": "Henri Beck <henribeck.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=\"test\" ava",
    "test:watch": "yarn run test -- --watch",
    "test:coverage": "nyc yarn run test",
    "build": "babel src --out-dir lib",
    "generate-report:html": "nyc report --reporter=html",
    "generate-report:lcov": "nyc report --reporter=text-lcov",
    "report-coverage": "yarn run generate-report:lcov | coveralls",
    "prepublish": "yarn run build",
    "lint": "eslint src/index.js",
    "precommit": "yarn run lint"
  },
  "devDependencies": {
    "@tf2-pickup/eslint-config": "2.7.7",
    "ava": "0.20.0",
    "babel-cli": "6.24.1",
    "babel-env": "2.4.1",
    "babel-register": "6.24.1",
    "coveralls": "2.13.1",
    "eslint": "4.1.1",
    "husky": "0.14.2",
    "nyc": "11.0.3"
  },
  "ava": {
    "files": [
      "src/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": {
      "babelrc": true
    }
  },
  "nyc": {
    "check-coverage": true,
    "exclude": [
      "src/*.spec.js"
    ],
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "report-dir": "./.coverage"
  }
}
